<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.xie.myandroidchart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/android_chart_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/android_chart_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AndroidLineChartActivity" />
        <activity android:name=".activity.LineChartBaseActivity"/>
    </application>

</manifest>