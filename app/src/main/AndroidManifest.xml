<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.xie.myandroidchart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/android_chart_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/android_chart_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LineBasic2DActivity"></activity>
        <activity android:name=".activity.ColumnStacked3DActivity" />
        <activity android:name=".activity.ColumnStacked2DActivity" />
        <activity android:name=".activity.ColumnMulti3DActivity" />
        <activity android:name=".activity.ColumnMulti2DActivity" />
        <activity android:name=".activity.Column3DActivity" />
        <activity android:name=".activity.Column2DActivity" />
        <activity android:name=".activity.BarStacked2DActivity" />
        <activity android:name=".activity.BarMulti2DActivity" />
        <activity android:name=".activity.Bar2DActivity" />
        <activity android:name=".activity.Area2DActivity" />
        <activity android:name=".activity.IChartJSActivity" />
        <activity android:name=".activity.AndroidIChartJSActivity" />
        <activity android:name=".activity.Android3DCompassActivity" />
        <activity android:name=".activity.BarChartBaseActivity" />
        <activity android:name=".activity.AndroidBarChartActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AndroidLineChartActivity" />
        <activity android:name=".activity.LineChartBaseActivity" />
    </application>

</manifest>