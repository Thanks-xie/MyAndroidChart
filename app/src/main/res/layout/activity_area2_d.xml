<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.Area2DActivity">

    <include android:id="@+id/nav_bar"
        layout="@layout/action_bar"/>

    <!--单一数据源-->
    <WebView
        android:id="@+id/webView_area2d"
        android:layout_below="@+id/nav_bar"
        android:layout_margin="10dp"
        android:background="@android:color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <!--多数据源-->
    <WebView
        android:id="@+id/webView_area2d_02"
        android:layout_below="@+id/webView_area2d"
        android:layout_margin="10dp"
        android:background="@android:color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <!--多点重合数据源-->
    <WebView
        android:id="@+id/webView_area2d_03"
        android:layout_below="@+id/webView_area2d_02"
        android:layout_margin="10dp"
        android:background="@android:color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</RelativeLayout>